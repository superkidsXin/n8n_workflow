{
  "name": "n8n-nodes-bugly",
  "version": "0.1.0",
  "description": "n8n custom node for fetching Bugly crash details",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rmdir /s /q dist 2>nul || exit 0",
    "test:bugly": "node scripts/test-bugly.cjs"
  },
  "peerDependencies": {
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.0.0",
    "typescript": "^5.5.4"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BuglyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Bugly/Bugly.node.js",
      "dist/nodes/CrashParse/CrashParse.node.js"
    ]
  }
}